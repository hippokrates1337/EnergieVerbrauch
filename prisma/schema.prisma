// Learn more about this file in the docs at https://pris.ly/d/prisma-schema
datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native"]
}

model User {
    uid String @id @default(cuid())
    name String
    email String
    password String
    createdAt DateTime
}